{
  "expo": {
    "name": "Crowpedia",
    "slug": "Crowpedia",
    "version": "0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "crowpedia",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.suspectcrow.Crowpedia"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "resizeMode": "cover",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          },
          "enableFullScreenImage_legacy": true
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/DMSans-Black.ttf",
            "./assets/fonts/DMSans-Bold.ttf",
            "./assets/fonts/DMSans-ExtraBold.ttf",
            "./assets/fonts/DMSans-ExtraLight.ttf",
            "./assets/fonts/DMSans-Light.ttf",
            "./assets/fonts/DMSans-Medium.ttf",
            "./assets/fonts/DMSans-Regular.ttf",
            "./assets/fonts/DMSans-SemiBold.ttf",
            "./assets/fonts/DMSans-Thin.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
